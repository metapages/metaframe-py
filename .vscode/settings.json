{
    "deno.enable": true,
    "deno.unstable": true,
    "deno.enablePaths": [
        "server"
    ],
    "files.exclude": {
        "**/node_modules": true,
        "client/dist": true,
        "server/editor/assets": true
    },
    "files.watcherExclude": {
        "server/editor/assets": true
    },
    "search.exclude": {
        "client/dist": true,
        "server/editor/assets": true
    },
    "css.format.enable": false

}
